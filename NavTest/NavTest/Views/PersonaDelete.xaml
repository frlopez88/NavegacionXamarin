             x:Class="NavTest.Views.PersonaDelete"
             Title="Delete">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Ingrese Id a Borrar"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
